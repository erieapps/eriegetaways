          "_customHTML": "<section data-bs-version=\"5.1\" class=\"features4\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Blurred Circle\" name=\"showBlurredCircle\" checked condition=\"cardsCount>2\">\n        <input type=\"color\" name=\"circleColor1\" title=\"Circle Color 1\" value=\"#FFF182\" condition=\"showBlurredCircle &amp;&amp; cardsCount>2\">\n        <input type=\"color\" name=\"circleColor2\" title=\"Circle Color 2\" value=\"#77BBDF\" condition=\"showBlurredCircle &amp;&amp; cardsCount>2\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Comment\" name=\"showComment\">\n        <input type=\"checkbox\" title=\"Number\" name=\"showNumber\">\n        <input type=\"color\" name=\"numberColor\" title=\"Number Color\" value=\"#2A2A2A\" condition=\"showNumber\">\n        <input type=\"checkbox\" title=\"Icons\" name=\"showIcon\" checked condition=\"cardsCount>2\">\n        <input type=\"color\" title=\"Icons Bg Color\" name=\"IconBg\" value=\"#5F6AD4\" condition=\"showIcon &amp;&amp; cardsCount>2\">\n        <input type=\"