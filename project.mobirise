              }
            },
            ".mbr-text": {
              "color": "#2A2A2A",
              "flex-grow": "1",
              "text-align": "center"
            },
            ".mbr-phone": {
              "color": "#2A2A2A",
              "flex-grow": "1"
            },
            ".comment-text": {
              "width": "100%",
